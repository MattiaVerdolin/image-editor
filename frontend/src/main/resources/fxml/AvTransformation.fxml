<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Label?>
<?import javafx.geometry.Insets?>

<VBox fx:id="vbox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" spacing="10.0">
    <Label fx:id="avTransformationLabel" text="Available transformations" VBox.vgrow="ALWAYS">
        <font>
            <Font name="Montserrat SemiBold" size="14.0" />
        </font>
        <VBox.margin>
            <Insets left="20.0" top="15.0" />
        </VBox.margin>
    </Label>
    <ListView fx:id="transformations_ListView" prefHeight="250.0"/>
</VBox>
